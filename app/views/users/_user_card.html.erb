<%= link_to user_path(cook) do %>
<li>
  <div class="flex items-center gap-x-6">
    <%= image_tag avatar_url(cook), class: "h-20 w-20 rounded-full" %>
    <div>
      <h3 class="text-base font-semibold leading-7 tracking-tight text-gray-900"><%= cook.full_name %></h3>
      <p class="text-sm font-semibold leading-6 text-indigo-600"><%= cook.title %></p>
    </div>
  </div>
</li>
<% end %>